{
  "functions": {
    "app/api/notifications/stream/route.ts": {
      "maxDuration": 300
    }
  },
  "headers": [
    {
      "source": "/api/notifications/stream",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-transform"
        },
        {
          "key": "Connection",
          "value": "keep-alive"
        }
      ]
    }
  ]
}
